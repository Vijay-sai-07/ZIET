SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc1.Calories, 0) AS BreakfastCalories FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc1 ON u.User_id = mc1.User_id AND mc1.Meal_Type = 'BreakFast' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id WHERE u.Status = 'Active'
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 1
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Good source of carbohydrates and fiber.",      "food_item": "Masala Rava Kichadi"    },    {      "quantity_value": 2,      "quantity_type": "nos",      "food_description": "Excellent source of protein.",      "food_item": "Boiled Egg"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in carbohydrates and fiber.",      "food_item": "Sambar"    }  ],  "meal_description": "A balanced breakfast with protein, carbohydrates, and fiber.",  "total_calories": 340.0,  "total_proteins": 15.0,   "total_carbohydrates": 60.0,  "total_fats": 10.0,  "total_fibres": 5.0 }', 1)
INSERT INTO user_responses (User_id, response, mealID) VALUES  (2, '{  "meal_plan": [    {      "quantity_value": 1,      "quantity_type": "nos",      "food_description": "Good source of carbohydrates and protein.",      "food_item": "Idli"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in carbohydrates and fiber.",      "food_item": "Masala Rava Kichadi"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Provides healthy fats and fiber.",      "food_item": "Coconut Chutney"    }  ],  "meal_description": "A balanced breakfast with carbohydrates, protein, and healthy fats.",  "total_calories": 336.0,  "total_proteins": null,   "total_carbohydrates": null,  "total_fats": null,  "total_fibres": null}', 1)
INSERT INTO user_responses (User_id, response, mealID) VALUES  (3, '{  "meal_plan": [    {      "quantity_value": 1,      "quantity_type": "nos",      "food_description": "Good source of carbohydrates and protein.",      "food_item": "Idli"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in carbohydrates and fiber.",      "food_item": "Sambar"    }  ],  "meal_description": "A balanced breakfast with carbohydrates, protein, and fiber.",  "total_calories": 120.0,  "total_proteins": 0.0,   "total_carbohydrates": 0.0,  "total_fats": 0.0,  "total_fibres": 0.0 }', 1)
SELECT * FROM user_responses WHERE DATE(response_date_time) = CURDATE() AND mealID = 1
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 2
SELECT EmailId FROM user WHERE User_id = 3
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 2
SELECT EmailId FROM user WHERE User_id = 3
