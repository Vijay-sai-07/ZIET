SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc4.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc4 ON u.User_id = mc4.User_id AND mc4.Meal_Type = 'Dinner' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc4.Calories
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 4
