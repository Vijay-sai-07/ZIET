SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc5.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc5 ON u.User_id = mc5.User_id AND mc5.Meal_Type = 'Midnight_Dinner' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc5.Calories
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 5
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "quantity_value": 1,      "quantity_type": "nos",      "food_description": "Good source of protein and carbohydrates",      "food_item": "Dosa"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in carbohydrates and some vitamins",      "food_item": "White Rice"    },    {      "quantity_value": 1,      "quantity_type": "nos",      "food_description": "Good source of protein",      "food_item": "Boiled Egg"    }  ],  "meal_description": "A balanced meal providing carbohydrates, protein, and some vitamins.",  "total_calories": 301.0,  "total_proteins": 8.0,   "total_carbohydrates": 60.0,   "total_fats": 10.0,   "total_fibres": 2.0 }', 5)
INSERT INTO user_responses (User_id, response, mealID) VALUES  (2, '{  "meal_plan": [    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Good source of Vitamin C and fiber",      "food_item": "Guava"    },    {      "quantity_value": 1,      "quantity_type": "nos",      "food_description": "Rich in carbohydrates and proteins",      "food_item": "Boiled Egg"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Provides carbohydrates and fiber",      "food_item": "White Rice"    }  ],  "meal_description": "A balanced meal with carbohydrates, protein, and vitamins.",  "total_calories": 295.0,  "total_proteins": 6.0,   "total_carbohydrates": 60.0,  "total_fats": 5.0,  "total_fibres": 5.0 }', 5)
INSERT INTO user_responses (User_id, response, mealID) VALUES  (3, '{  "meal_plan": [    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Good source of carbohydrates and Vitamin C",      "food_item": "White Rice"    },    {      "quantity_value": 2,      "quantity_type": "nos",      "food_description": "High in protein and essential nutrients.",      "food_item": "Boiled Egg"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in carbohydrates and fiber",      "food_item": "Carrot Green Peas Poriyal"    }  ],  "meal_description": "A balanced meal with carbohydrates, protein, and some vitamins.",  "total_calories": 341.0,  "total_proteins": 15.0,   "total_carbohydrates": 60.0,  "total_fats": 10.0,  "total_fibres": 5.0 }', 5)
SELECT * FROM user_responses WHERE DATE(response_date_time) = CURDATE() AND mealID = 5
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 2
SELECT EmailId FROM user WHERE User_id = 3
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 2
SELECT EmailId FROM user WHERE User_id = 3
TRUNCATE TABLE food_items
SELECT AFoodName FROM all_foods
