SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc1.Calories, 0) AS BreakfastCalories FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc1 ON u.User_id = mc1.User_id AND mc1.Meal_Type = 'BreakFast' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id WHERE u.Status = 'Active'
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 1
