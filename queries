SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc1.Calories, 0) AS BreakfastCalories FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc1 ON u.User_id = mc1.User_id AND mc1.Meal_Type = 'BreakFast' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id WHERE u.Status = 'Active'
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 1
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "food_description": "Good source of carbohydrates and protein.",      "quantity_value": 2,      "quantity_type": "nos",      "food_item": "Idli"    },    {      "food_description": "Rich in carbohydrates and fiber.",      "quantity_value": 100,      "quantity_type": "gms",      "food_item": "Masala Rava Kichadi"    },    {      "food_description": "Protein-rich and healthy fats.",      "quantity_value": 2,      "quantity_type": "nos",      "food_item": "Boiled Egg"    }  ],  "meal_description": "A balanced breakfast with carbohydrates, protein, and healthy fats.",  "total_calories": 426.0,  "total_proteins": 20.0,   "total_carbohydrates": 60.0,   "total_fats": 15.0,   "total_fibres": 5.0 }', 1)
