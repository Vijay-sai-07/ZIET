SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc2.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc2 ON u.User_id = mc2.User_id AND mc2.Meal_Type = 'Lunch' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc2.Calories
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 2
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "quantity_value": 200,      "quantity_type": "gms",      "food_description": "Good source of carbohydrates and protein",      "food_item": "Pongal"    },    {      "quantity_value": 150,      "quantity_type": "gms",      "food_description": "Rich in calcium and probiotics",      "food_item": "Curd Rice"    },    {      "quantity_value": 2,      "quantity_type": "nos",      "food_description": "Excellent source of protein",      "food_item": "Boiled Egg"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Provides essential vitamins and minerals",      "food_item": "Gram Dal Thuvaiyal"    }  ],  "meal_description": "A balanced meal rich in protein, carbohydrates, and healthy fats.",  "total_calories": 812.0,  "total_proteins": 30.0,   "total_carbohydrates": 100.0,  "total_fats": 30.0,  "total_fibres": 10.0}', 2)
SELECT * FROM user_responses WHERE DATE(response_date_time) = CURDATE() AND mealID = 2
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 1
SELECT EmailId FROM user WHERE User_id = 1
SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc3.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc3 ON u.User_id = mc3.User_id AND mc3.Meal_Type = 'Snack' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc3.Calories
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 3
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "quantity_value": 1,      "quantity_type": "nos",      "food_description": "Good source of carbohydrates and protein",      "food_item": "Veg Cutlet"    },    {      "quantity_value": 50,      "quantity_type": "gms",      "food_description": "Rich in vitamins and antioxidants",      "food_item": "Mint Chutney"    }  ],  "meal_description": "Provides carbohydrates, protein, vitamins and antioxidants",  "total_calories": 111.5,  "total_proteins": 5.0,   "total_carbohydrates": 20.0,   "total_fats": 5.0,   "total_fibres": 2.0 }', 3)
SELECT * FROM user_responses WHERE DATE(response_date_time) = CURDATE() AND mealID = 3
SELECT EmailId FROM user WHERE User_id = 1
