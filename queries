SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc5.Calories, 0) AS MidnightDinnerCalories FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc5 ON u.User_id = mc5.User_id AND mc5.Meal_Type = 'Midnight_Dinner' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id WHERE u.Status = 'Active'
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 5
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in Vitamin C and antioxidants",      "food_item": "Pine Apple fruit"    },    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Good source of Vitamin C and folate",      "food_item": "Orange Juice"    }  ],  "meal_description": "A light and refreshing meal with Vitamin C and antioxidants.",  "total_calories": 140.0,  "total_proteins": 0.0,   "total_carbohydrates": 0.0,   "total_fats": 0.0,   "total_fibres": 0.0 }', 5)
