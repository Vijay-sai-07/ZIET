SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc5.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc5 ON u.User_id = mc5.User_id AND mc5.Meal_Type = 'Midnight_Dinner' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc5.Calories
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 5
INSERT INTO user_responses (User_id, response, mealID) VALUES  (1, '{  "meal_plan": [    {      "food_description": "Good source of carbohydrates and fiber.",      "quantity_value": 100,      "quantity_type": "gms",      "food_item": "White Rice"    },    {      "food_description": "Rich in protein.",      "quantity_value": 2,      "quantity_type": "nos",      "food_item": "Boiled Egg"    },    {      "food_description": "Source of carbohydrates and vitamins.",      "quantity_value": 100,      "quantity_type": "gms",      "food_item": "Tomato Pappu"    }  ],  "meal_description": "A balanced meal providing carbohydrates, protein, and some vitamins.",  "total_calories": 331.0,  "total_proteins": 12.0,   "total_carbohydrates": 60.0,  "total_fats": 10.0,  "total_fibres": 5.0 }', 5)
INSERT INTO user_responses (User_id, response, mealID) VALUES  (2, '{  "meal_plan": [    {      "quantity_value": 100,      "quantity_type": "gms",      "food_description": "Rich in Vitamin C and antioxidants",      "food_item": "Watermelon Juice"    },    {      "quantity_value": 50,      "quantity_type": "gms",      "food_description": "Good source of carbohydrates and fiber",      "food_item": "White Rice"    },    {      "quantity_value": 50,      "quantity_type": "gms",      "food_description": "Source of protein and calcium",      "food_item": "Curd"    }  ],  "meal_description": "A balanced meal with carbohydrates, some protein, and vitamins.",  "total_calories": 144.5,  "total_proteins": 5.0,   "total_carbohydrates": 30.0,  "total_fats": 2.0,  "total_fibres": 2.0 }', 5)
SELECT * FROM user_responses WHERE DATE(response_date_time) = CURDATE() AND mealID = 5
SELECT EmailId FROM user WHERE User_id = 1
