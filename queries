INSERT INTO food_items(Sid, mealID, FoodName, Quantity_type, Quantity_value, Calories,Contains) VALUES (10,1,'Kiran ','gms',100.00,30,'null')
SELECT * FROM all_foods WHERE AFoodName = 'Kiran '
SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc2.Calories, 0) AS LunchCalories FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc2 ON u.User_id = mc2.User_id AND mc2.Meal_Type = 'Lunch' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id WHERE u.Status = 'Active'
SELECT CONCAT(f.FoodName, ':', f.Calories, ' kcal per ', f.Quantity_value, ' ', f.Quantity_type, ' ', m.meal) AS food_details FROM food_items f JOIN meal m ON f.mealID = m.mealID WHERE f.mealID = 2
