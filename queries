SELECT * FROM user_responses WHERE DATE(response_date_time) = CURDATE() AND mealID = 5
SELECT EmailId FROM user WHERE User_id = 1
TRUNCATE TABLE food_items
SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc1.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc1 ON u.User_id = mc1.User_id AND mc1.Meal_Type = 'BreakFast' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc1.Calories
TRUNCATE TABLE food_items
SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc2.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc2 ON u.User_id = mc2.User_id AND mc2.Meal_Type = 'Lunch' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc2.Calories
TRUNCATE TABLE food_items
SELECT     u.User_id,     u.age,     u.height,     u.weight,     up.isVeg,     up.fitness_goals,     up.Dietary_preference,COALESCE(mc5.Calories, 0) AS Calories_per_meal, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'FAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS FavouriteFoods, COALESCE(NULLIF(GROUP_CONCAT(DISTINCT CASE WHEN uf.Favourite_status = 'UNFAV' THEN uf.Food END ORDER BY uf.Food SEPARATOR ', '), ''), 'None') AS UnfavouriteFoods FROM user u LEFT JOIN UserPreferences up ON u.User_id = up.User_id LEFT JOIN MealCalories mc5 ON u.User_id = mc5.User_id AND mc5.Meal_Type = 'Midnight_Dinner' LEFT JOIN userDiseases ud ON u.User_id = ud.User_Id LEFT JOIN userFavourites uf ON u.User_id = uf.User_id WHERE u.Status = 'Active' GROUP BY u.User_id, u.age, u.height, u.weight, up.isVeg, up.fitness_goals, up.Dietary_preference, mc5.Calories
TRUNCATE TABLE food_items
